{
  "root": "./",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "theme-default",
    "search",
    "lunr",
    "sharing",
    "fontsettings",
    "highlight",
    "mermaid-gb3"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
